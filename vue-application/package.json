{
  "name": "{{productName}}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "check-types": "npx vue-tsc --noEmit",
    "verify": "npm run lint && npm run check-types && npm run test",
    "test": "vitest run",
    "check-dependencies": "npx npm-check-updates",
    "update-dependencies": "npx npm-check-updates -u",
    "generate-view-examples": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateViewExamples",
    "generate-icons": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateIcons"
  },
  "dependencies": {
    "vue": "^3.4.25",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "npm-check-updates": "^16.14.20",
    "sass": "^1.75.0",
    "svgo": "^3.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-vue-examples": "^0.0.4",
    "vitest": "^1.5.2",
    "vue-tsc": "^2.0.14"
  }
}
