{
  "name": "{{productName}}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "check-types": "npx vue-tsc --noEmit",
    "verify": "npm run lint && npm run check-types && npm run test",
    "test": "vitest run",
    "generate-view-examples": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateViewExamples",
    "generate-icons": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateIcons"
  },
  "dependencies": {
    "vitest": "^0.24.3",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "npm-check-updates": "^16.3.13",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "svgo": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.8"
  }
}
