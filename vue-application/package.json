{
  "name": "{{productName}}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "test": "jest",
    "viewExamples": "vite -c vite.viewExamples.config.ts --port 3001",
    "generateViewExamples": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateViewExamples",
    "generateIcons": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/generateIcons"
  },
  "dependencies": {
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "svgo": "^2.8.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.32.1"
  }
}
