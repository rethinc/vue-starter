    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "svgo": "^2.8.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.4",
